<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Strict//EN"
    "http://www.w3.org/TR/xhtml1/DTD/xhtml1-strict.dtd">
<html xmlns="http://www.w3.org/1999/xhtml" lang="en" xml:lang="en">
<head>
  <link href="/stylesheets/screen.css" media="screen, projection" rel="stylesheet" type="text/css" />
  <link href="/stylesheets/print.css" media="print" rel="stylesheet" type="text/css" />
<!--[if lt IE 8]>
    <link href="/stylesheets/ie.css" media="screen, projection" rel="stylesheet" type="text/css" />
<![endif]-->
  <title>
    Plate Viewer
  </title>
</head>
<body>
  <div class="container">
    <div id="header">
      <div id="header-top">
        <div id="quick-links">
            <a href="http://intweb/Software/launch">Wellcome Trust Sanger Institute</a>
        </div>
        <div id="user-links">Welcome Frank Sinatra &bull; Logout</div>
      </div> <!-- End of header-top -->
      
      <div id="header-middle">
        <div id="logo-container">
          <img id="logo" src="/images/sequencescape_logo_new.png"/>
          <p id="application-name">Sequencescape</p>
        </div> <!-- End of logo-container -->
        
        <div id="search">
          <form method="get" action="/searches">
            <ul>
              <li><label for="layout_search">Search</label></li>
              <li><input id="layout_search" type="text" value="" size="20" name="q" /></li>
            </ul>
            
            
          </form>
        </div> <!-- End of search -->
      </div> <!-- End of header-middle -->
        
      
      <div id="main-menu-container">
        <ul id="main-menu">
          <li><a href="#">Studies</a></li>
          <li><a href="#">Projects</a></li>
          <li><a href="#">Pipelines</a></li>
          <li><a href="#">Batches</a></li>
          <li><a href="#">Reception</a></li>
          <li><a href="#">Lab View</a></li>
          <li><a href="#">Lab Search</a></li>
        </ul>
      </div> <!-- End of main-main-container -->
    </div> <!-- End of header -->
    <div id="main-content">
      <div id="yield">
        <%= yield %>
      </div>
      
    </div>
  </div> <!-- End of container -->
</body>
</html>
